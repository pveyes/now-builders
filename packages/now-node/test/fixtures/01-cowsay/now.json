{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@now/node" },
    { "src": "subdirectory/index.js", "use": "@now/node" }
  ],
  "probes": [
    { "path": "/", "body": {}, "mustContain": "cow:RANDOMNESS_PLACEHOLDER" },
    {
      "path": "/subdirectory/",
      "body": {},
      "mustContain": "yoda:RANDOMNESS_PLACEHOLDER"
    }
  ]
}
